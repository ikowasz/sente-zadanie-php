{% extends 'base.html.twig' %}

{% block body %}
<div class="container">

  <table class="table table-striped text-center">
    <thead>
      <tr>
        {% for row in rows %}
          <th scope="col">{{ row }}</th>
        {% endfor %}
      </tr>
    </thead>

    <tbody>
      {% for order in orders %}
      <tr>
        {% for row in rows %}
          <td>
            {% if attribute(order, row).timestamp is defined %}
              {{ attribute(order, row)|date('Y-m-d H:i') }}
            {% else %}
              {{ attribute(order, row) }}
            {% endif %}
          </td>
        {% endfor %}
      </tr>
      {% endfor %}
    </tbody>
  </table>

</div>
{% endblock %}